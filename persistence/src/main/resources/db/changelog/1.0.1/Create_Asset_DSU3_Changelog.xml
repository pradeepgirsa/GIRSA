            <column name="AMOUNT_ISSUED" type="NUMERIC(28,  10)"/>
            <column name="SUB_SECTOR" type="varchar(255)"/>
            <column name="GICS_INDUSTRY_GROUP" type="varchar(255)"/>
            <column name="TIME_TO_MATURITY" type="varchar(255)"/>
            <column name="COUNTRY_OF_EXPOSURE" type="varchar(255)"/>
            <column name="COUNTRY_OF_INCORPORATION" type="varchar(255)"/>
            <column name="COUNTRY_OF_QUOTATION" type="varchar(255)"/>
            <column name="MOODY_RATING" type="varchar(255)"/>
            <column name="PARENT_ISSUER_ID" type="varchar(255)"/>
            <column name="ISSUER_SHORT_NAME" type="varchar(255)"/>
            <column name="NET_INDICATOR" type="boolean"/>

        </createTable>

        <addAutoIncrement columnDataType="bigint"
                          columnName="id"
                          incrementBy="1"
                          startWith="100"
                          tableName="barra_asset_dsu3"/>
    </changeSet>

</databaseChangeLog>