<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="barra_asset_dsu4_table_1" author="Sharanya">
        <createTable tableName="barra_asset_dsu4" >
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="ASSET_ID" type="varchar(255)"/>
            <column name="ASSET_ID_TYPE" type="varchar(255)"/>
            <column name="ASSET_NAME" type="varchar(255)"/>
            <column name="AFRICA_VALUES" type="NUMERIC(28,  10)"/>
            <column name="EFF_EXPOSURE" type="NUMERIC(28,  10)"/>
            <column name="MARKET_VALUE" type="NUMERIC(28,  10)"/>
            <column name="EFF_WEIGHT" type="NUMERIC(28,  10)"/>
            <column name="GIR_ISSUER" type="varchar(255)"/>
            <column name="HOLDINGS" type="NUMERIC(28,  10)"/>
            <column name="INST_TYPE" type="varchar(255)"/>
            <column name="INST_SUB_TYPE" type="varchar(255)"/>
            <column name="LOCAL_MARKET" type="varchar(255)"/>
            <column name="PRICE" type="NUMERIC(28,  10)"/>
            <column name="REG28INSTR_TYPE" type="varchar(255)"/>
            <column name="SARB_CLASSIFICATION" type="varchar(255)"/>
            <column name="PRICE_CURRENCY" type="varchar(255)"/>
            <column name="NET_INDICATOR" type="boolean"/>

        </createTable>

        <addAutoIncrement columnDataType="bigint"
                          columnName="id"
                          incrementBy="1"
                          startWith="100"
                          tableName="barra_asset_dsu4"/>
    </changeSet>

</databaseChangeLog>